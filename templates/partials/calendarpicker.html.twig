<script await src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/js/bootstrap-datepicker.min.js"></script>
<script>
    $(document).ready(function(){
        $('#booking_startDate').datepicker({
            format: 'dd/mm/yyyy',
            datesDisabled:[
                {% for day in NotAvailableDays %}
                "{{ day.format('d/m/Y') }}",

                {% endfor %}

            ],
            startDate: new Date()
        });
        $('#booking_endDate ').datepicker({
            format: 'dd/mm/yyyy',
            datesDisabled:[
                {% for day in NotAvailableDaysEnd %}
                "{{ day.format('d/m/Y') }}",

                {% endfor %}

            ],
            startDate: new Date()
        });
    
    });
</script>